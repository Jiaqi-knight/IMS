Cell[CellGroupData[{

Cell["Imtek`Voronoi`", "Section",
  CellDingbat->"\[FilledSquare]"],

Cell["\<\
This package provides utilities for computing Voronoi tesselations \
of triangular grids. \
\>", "Text"],

Cell[BoxData[GridBox[{
        {
          RowBox[{
            RowBox[{"\"\<imsVoronoiEdgeLength[{\!\(\*
StyleBox[\(pt\_i\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(pt\_j\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(pt\_k\),\\nFontSlant->\\\"Italic\\\"]\)}\>\"", ",", " ", 
              RowBox[{"{", 
                StyleBox[\(pt\_a\),
                  FontSlant->"Italic"], "}"}]}], 
            "]"}], "\<\"gives three edge length {\!\(\*
StyleBox[\(a\_ij\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(a\_jk\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(a\_ki\),\\nFontSlant->\\\"Italic\\\"]\)} from mid point of the \
three lines to the fourth given point \!\(\*
StyleBox[\(pt\_a\),\\nFontSlant->\\\"Italic\\\"]\).\"\>"},
        {"\"\<imsVoronoiEdgeUnitNormal[{\!\(\*
StyleBox[\(pt\_i\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(pt\_j\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(pt\_k\),\\nFontSlant->\\\"Italic\\\"]\)}]\>\"", "\<\"gives a  list \
containing the three edge normal vectors {\!\(\*
StyleBox[\(n\_ij\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(n\_jk\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(n\_ki\),\\nFontSlant->\\\"Italic\\\"]\)} to the Voronoi edge \
length {\!\(\*
StyleBox[\(a\_ij\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(a\_jk\),\\nFontSlant->\\\"Italic\\\"]\), \!\(\*
StyleBox[\(a\_ki\),\\nFontSlant->\\\"Italic\\\"]\)}.\"\>"}
        }]], "DefinitionBox"],

Cell["This loads the package", "Text",
  CellDingbat->"\[FilledSmallSquare]"],

Cell[BoxData[
    \(Needs[\ "\<Imtek`Voronoi`\>"]\)], "Input",
  CellLabel->"In[1]:="],

Cell["\<\
For the moment the Voronoi package can only handle triangles. We \
need\
\>", "Text"],

Cell[BoxData[
    \(Needs[\ "\<Imtek`Triangle`\>"]\)], "Input",
  CellLabel->"In[2]:="],

Cell[CellGroupData[{

Cell[BoxData[{
    \(\(i = {0, 0};\)\), "\[IndentingNewLine]", 
    \(\(j = {2, 0};\)\), "\[IndentingNewLine]", 
    \(\(k = {0.5, \ 1};\)\), "\[IndentingNewLine]", 
    \(\(triAng := {i, \ j, \ k};\)\), "\[IndentingNewLine]", 
    \(\(\(mp = imsCircumcenter[\ triAng\ ];\)\(\[IndentingNewLine]\)
    \)\), "\[IndentingNewLine]", 
    \(\(addPos = {0.05, 0.05};\)\), "\[IndentingNewLine]", 
    \(Show[\ 
      Graphics[{\[IndentingNewLine]imsTriangle[\ {i, j, \ k\ }\ ], \ 
          Point[mp], \[IndentingNewLine]Text["\<i\>", \ i + addPos], 
          Text["\<j\>", \ j + addPos], \[IndentingNewLine]Text["\<k\>", \ 
            k + addPos], 
          Text["\<mp\>", \ mp\  + \ addPos]\[IndentingNewLine]}]\ , 
      AspectRatio \[Rule] Automatic\ ]\)}], "Input",
  CellLabel->"In[3]:="],

