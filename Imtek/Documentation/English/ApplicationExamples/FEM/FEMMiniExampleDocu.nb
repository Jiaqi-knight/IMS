Cell[BoxData[{
    \(numberOfNodes\  = \ 
      Length[\ boundaryNodes\ ]\  + \ 
        Length[\ interiorNodes\ ]\), "\[IndentingNewLine]", 
    \(numberOfElements\  = \ Length[\ allElements\ ]\)}], "Input",
  CellLabel->"In[24]:="],

