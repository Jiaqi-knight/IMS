Cell[TextData[StyleBox["Dbg \[Dash] an easy to use debugger",
  FontColor->RGBColor[0, 0, 1]]], "Chapter"],

Cell[BoxData[{
    RowBox[{"David", " ", "Bailey"}], "\n", 
