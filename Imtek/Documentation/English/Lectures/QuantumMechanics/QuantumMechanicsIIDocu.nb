Cell[CellGroupData[{

Cell[TextData[StyleBox["Look at some plots of Spherical Harmonics !",
  FontColor->RGBColor[1, 0, 0]]], "Subsubsection"],

Cell[TextData[{
  "This is a plot of the absolute value of ",
  Cell[BoxData[
      \(TraditionalForm\`Y\_\(l, m\)\)]],
  ". The phase of ",
  Cell[BoxData[
      \(TraditionalForm\`Y\_\(l, m\)\)]],
  "is color coded:"
}], "Text"],

Cell[BoxData[{
    StyleBox[\(<< Graphics`ParametricPlot3D`\),
      FontSize->6], "\[IndentingNewLine]", 
    StyleBox[\(n = \[Pi]\/128; 
      Show[{Graphics[\(({Hue[#1\/\(2\ \[Pi] - n\)], 
